{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "downlevelIteration": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": [
    "src/**/*"
  ]
}
